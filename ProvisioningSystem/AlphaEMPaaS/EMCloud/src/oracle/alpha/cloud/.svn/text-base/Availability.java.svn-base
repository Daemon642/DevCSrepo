package oracle.alpha.cloud;

public class Availability {
    private String ProvisionValue;
    private String LocalPrimaryEnv;
    private String LocalStorage;
    private String LocalFailoverEnv;
    private String LocalFailoverStorage;
    private String RemoteFailoverEnv;
    private String RemoteFailoverStorage;
    
    public Availability() {
        super();
    }

    public void setProvisionValue(String ProvisionValue) {
        this.ProvisionValue = ProvisionValue;
    }

    public String getProvisionValue() {
        return ProvisionValue;
    }

    public void setLocalPrimaryEnv(String LocalPrimaryEnv) {
        this.LocalPrimaryEnv = LocalPrimaryEnv;
    }

    public String getLocalPrimaryEnv() {
        return LocalPrimaryEnv;
    }

    public void setLocalStorage(String LocalStorage) {
        this.LocalStorage = LocalStorage;
    }

    public String getLocalStorage() {
        return LocalStorage;
    }

    public void setLocalFailoverEnv(String LocalFailoverEnv) {
        this.LocalFailoverEnv = LocalFailoverEnv;
    }

    public String getLocalFailoverEnv() {
        return LocalFailoverEnv;
    }

    public void setLocalFailoverStorage(String LocalFailoverStorage) {
        this.LocalFailoverStorage = LocalFailoverStorage;
    }

    public String getLocalFailoverStorage() {
        return LocalFailoverStorage;
    }

    public void setRemoteFailoverEnv(String RemoteFailoverEnv) {
        this.RemoteFailoverEnv = RemoteFailoverEnv;
    }

    public String getRemoteFailoverEnv() {
        return RemoteFailoverEnv;
    }

    public void setRemoteFailoverStorage(String RemoteFailoverStorage) {
        this.RemoteFailoverStorage = RemoteFailoverStorage;
    }

    public String getRemoteFailoverStorage() {
        return RemoteFailoverStorage;
    }
    
    @Override
    public String toString () {
        return ("Availability = ProvisionValue:" + this.getProvisionValue());
    }
}
