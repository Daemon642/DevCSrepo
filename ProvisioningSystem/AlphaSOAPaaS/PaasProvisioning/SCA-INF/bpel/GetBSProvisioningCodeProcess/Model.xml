      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="getbsprovisioningcodeprocess_client" partnerLinkType="client:GetBSProvisioningCodeProcess" myRole="GetBSProvisioningCodeProcessProvider"/>
    <partnerLink name="GetBSProvisioningCode" partnerLinkType="ns1:GetBSProvisioningCode_plt" partnerRole="GetBSProvisioningCode_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:GetBSProvisioningCodeProcessRequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:GetBSProvisioningCodeProcessResponseMessage"/>
    <variable name="nodeCount" type="xsd:int"/>
    <variable name="InvokeGetProvisioningCode_InputVariable" messageType="ns1:GetBSProvisioningCodeSelect_inputParameters"/>
    <variable name="InvokeGetProvisioningCode_OutputVariable" messageType="ns1:PBusinessParamCollection_msg"/>
    <variable name="loopCount" type="xsd:int"/>
    <variable name="xmlPathCode" type="xsd:string"/>
    <variable name="xmlPathValue" type="xsd:string"/>
    <variable name="provisioningCode" type="xsd:string"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main" id="BpSeq0">

    <!-- Receive input from requestor. (Note: This maps to operation defined in GetBSProvisioningCodeProcess.wsdl) -->
    <receive name="receiveInput" partnerLink="getbsprovisioningcodeprocess_client" portType="client:GetBSProvisioningCodeProcess" operation="process" variable="inputVariable" createInstance="yes" id="BpRcv0"/>
    <assign name="AssignInput" id="BpAss0">
      <copy>
        <from variable="inputVariable" part="payload" query="/client:process/client:BusinessServiceId"/>
        <to variable="InvokeGetProvisioningCode_InputVariable" part="GetBSProvisioningCodeSelect_inputParameters" query="/ns2:GetBSProvisioningCodeSelect_PBusinessServiceIdInputParameters/ns2:PBusinessServiceId"/>
      </copy>
    </assign>
    <invoke name="InvokeGetProvisioningCode" bpelx:invokeAsDetail="no" inputVariable="InvokeGetProvisioningCode_InputVariable" outputVariable="InvokeGetProvisioningCode_OutputVariable" partnerLink="GetBSProvisioningCode" portType="ns1:GetBSProvisioningCode_ptt" operation="GetBSProvisioningCodeSelect" id="BpInv0"/>
    <assign name="AssignLoop" id="BpAss1">
      <copy>
        <from expression="count(bpws:getVariableData('InvokeGetProvisioningCode_OutputVariable','PBusinessParamCollection','/ns2:PBusinessParamCollection/ns2:PBusinessParam'))"/>
        <to variable="nodeCount"/>
      </copy>
      <copy>
        <from expression="0"/>
        <to variable="loopCount"/>
      </copy>
    </assign>
    <while name="While1" condition="bpws:getVariableData('loopCount') &lt; bpws:getVariableData('nodeCount')" id="BpWhl0">
      <sequence name="Sequence1" id="BpSeq1">
        <assign name="IncLoop" id="BpAss2">
          <copy>
            <from expression="bpws:getVariableData('loopCount') + 1"/>
            <to variable="loopCount"/>
          </copy>
          <copy>
            <from expression="concat ('/ns2:PBusinessParamCollection/ns2:PBusinessParam[',bpws:getVariableData('loopCount'),']/ns2:provisionCode')"/>
            <to variable="xmlPathCode"/>
          </copy>
          <copy>
            <from expression="concat ('/ns2:PBusinessParamCollection/ns2:PBusinessParam[',bpws:getVariableData('loopCount'),']/ns2:provisionValue')"/>
            <to variable="xmlPathValue"/>
          </copy>
          <copy>
            <from expression="bpws:getVariableData('InvokeGetProvisioningCode_OutputVariable','PBusinessParamCollection',bpws:getVariableData('xmlPathCode'))"/>
            <to variable="provisioningCode"/>
          </copy>
        </assign>
        <switch name="Switch1" id="BpSwt0">
          <case condition="contains( bpws:getVariableData('provisioningCode'), bpws:getVariableData('inputVariable','payload','/client:process/client:ProvisionCode'))">
            <bpelx:annotation>
              <bpelx:general>
                <bpelx:property name="userLabel">
                  <![CDATA[Value Match]]>
                </bpelx:property>
              </bpelx:general>
            </bpelx:annotation>
            <assign name="AssignProvisionValue" id="BpAss3">
              <copy>
                <from expression="bpws:getVariableData('InvokeGetProvisioningCode_OutputVariable','PBusinessParamCollection',bpws:getVariableData('xmlPathValue'))"/>
                <to variable="outputVariable" part="payload" query="/client:processResponse/client:ProvisionValue"/>
              </copy>
              <copy>
                <from variable="nodeCount"/>
                <to variable="loopCount"/>
              </copy>
            </assign>
          </case>
          <otherwise>
            <assign name="AssignDefault" id="BpAss4">
              <copy>
                <from expression="'csbduser_us@oracle.com'"/>
                <to variable="outputVariable" part="payload" query="/client:processResponse/client:ProvisionValue"/>
              </copy>
            </assign>
          </otherwise>
        </switch>
      </sequence>
    </while>
    <!-- Generate reply to synchronous request -->
    <reply name="replyOutput" partnerLink="getbsprovisioningcodeprocess_client" portType="client:GetBSProvisioningCodeProcess" operation="process" variable="outputVariable" id="BpRpl0"/>
  </sequence>
