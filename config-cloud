Host AlphaDBCS
  hostname alphadbcs

  user oracle
  IdentityFile /u01/OPCWorkshop/lab/labkey

  # Database Access
  LocalForward 1521 alphadbcs:1521

  # Apex and DB Monitor
  # https://localhost:443/apex/apex/pdb1 .... workspace=internal, username=admin
  # https://localhost:443/dbaas_monitor ..... username=dbaas_monitor
  LocalForward 443 alphadbcs:443

  # Glass Fish
  # https://localhost:4848  .... username=admin
  LocalForward 4848 alphadbcs:4848

  # Enterprise Manager
  # https://localhost:5500/em  ..... username=sys
  LocalForward 5500 alphadbcs:5500

  ServerAliveInterval 60


Host AlphaJCS
  hostname alphajcs

  user opc
  IdentityFile /u01/OPCWorkshop/lab/labkey

  # WLS Admin Server
  LocalForward 9001 alphajcs:9001
  # VNC
  LocalForward 5901 alphajcs:5901

  RemoteForward 4000 localhost:7101

  ServerAliveInterval 60

